<div class="jumbotron">
  <h1>Dictionary</h1>
  <h3>$word.getWordName()</h3>
</div>
<div class="row">
  <div class="col-md-4">
    <a class="btn btn-primary" href="/words/$word.getId()/definitions/new">Add a new Definition</a>
    <h2>Definition:</h2>
    #foreach($definition in $word.getDefinitions())
    <div class="well">
      <h3>Definition:</h3><h4>$definition.getMeaning()</h4>
    </div>
    #end
  </div>

<br>
<a class="btn btn-info form-control" href="/words">View all words</a>
